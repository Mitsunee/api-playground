  "dependencies": {
    "node-fetch": "^3.1.0"
  },
  "devDependencies": {
    "@foxkit/eslint-config": "^1.0.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.0",
    "simple-git-hooks": "^2.7.0"
  }
}
